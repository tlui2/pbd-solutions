public class UsingVariables {
	public static void main(String[] args) {
	String learn = "Computer Science";
	int room = 113;
	double euler = 2.71828;
	
	    System.out.println("This is room " + room + ".");
	    System.out.println("e is close to " + euler + ".");
	    System.out.println("I am learning a bit about " + learn + ".");}
	    }
