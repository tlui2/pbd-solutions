public class Letter
{
        public static void main( String[] args)
        {
                System.out.println( "TTTTT  L" );
                System.out.println( "  T    L" );
                System.out.println( "  T    L" );
                System.out.println( "  T    L" );
                System.out.println( "  T    L" );
                System.out.println( "  T    L" );
                System.out.println( "  T    LLLLL" );
 		   }
}
